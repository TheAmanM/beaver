{
  "manifest_version": 3,
  "name": "Beaver",
  "version": "0.1",
  "description": "Make Acorn great again!",
  "permissions": ["tabs"],
  "host_permissions": ["*://acorn.utoronto.ca/sws/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://acorn.utoronto.ca/sws/*"],
      "js": ["content.js"]
    }
  ]
}
